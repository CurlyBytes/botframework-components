{
  "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-samples/main/experimental/generation/TestBot/TestBot/TestBot.schema",
  "$kind": "Microsoft.Test.Script",
  "httpRequestMocks": [
    "getProfile.mock",
    "getWorkingHours.mock",
    "getEvents.mock"
  ],
  "userTokenMocks": [
    "nomagiccode.mock"
  ],
  "dialog": "calendarskill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "Bot",
        "User"
      ],
      "membersRemoved": []
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "\r\nHi Megan Bowen üñê, thank you for helping the VA Skills team out with this dogfood for calendar skill!\r\n\r\nYou can ask the bot calendar related questions in this Teams chat modality (ps. other testing with modalities will follow like speaking to the bot).\r\n"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "where is my meeting with tom"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "I'm searching your calendar, this might take a sec‚Ä¶",
        "This might take a sec...",
        "Hold on, this is taking a while...",
        "Hold on, I'm trying to find events that match..."
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "I couldn't find the event that you're looking for. Let's try your search again. Can you tell me the specific title, start date, or attendees?",
        "I don't see a meeting with that info. Can you tell me more about the meeting you're looking for, like the start time, title of the event, or other attendees?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "with lidia"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "I'm searching your calendar, this might take a sec‚Ä¶",
        "This might take a sec...",
        "Hold on, this is taking a while...",
        "Hold on, I'm trying to find events that match..."
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "The meeting location is Conference Room - Stevens. The meeting is on Tuesday, October 15th. It‚Äôs at 8:00 AM, Project Team Meeting. The attendees are Lidia Holloway and 4 more. ",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.adaptive'"
      ]
    }
  ]
}
